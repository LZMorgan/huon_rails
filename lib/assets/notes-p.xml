                <note n="49" xml:id="p1052" type="edit" resp="#SPM">MS <hi rend="#italic"
                        >certanamt</hi></note>
                <note n="50" xml:id="p1057" type="ling" resp="#SPM"><hi rend="#italic">alquant</hi>;
                    rhoticism.</note>
