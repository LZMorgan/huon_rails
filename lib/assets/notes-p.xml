                <note xml:id="p1565" type="edit" resp="#SPM">MS <hi rend="#italic"
                    >cavalvaliro</hi>.</note>
                <note xml:id="p1598" type="edit" resp="#SPM">This line repeats.</note>
                <note xml:id="p1601" type="edit" resp="#SPM">MS <hi rend="#italic">Nia</hi></note>
                <note xml:id="p1625" type="edit" resp="scribe">MS <hi rend="#italic">l</hi>; scribal
                    correction with <hi rend="#italic">e</hi> written above following <hi
                        rend="#italic">v</hi>.</note>
                <note xml:id="p1636" type="codic" resp="#SPM">Space for illumination.</note>
                <note xml:id="p1647" type="edit" resp="scribe">MS <hi rend="#italic">liois</hi>;
                    scribe attempts to correct this word by adding an <hi rend="#italic">i</hi>
                    after the initial <hi rend="#italic">l</hi> and making the second <hi
                        rend="#italic">i</hi> an <hi rend="#italic">r</hi>.</note>
                <note xml:id="p1691" type="edit" resp="scribe">MS <hi rend="#italic">cholo</hi>;
                    scribal correction, with line drawn through top of first <hi rend="#italic"
                        >o</hi> to make it an <hi rend="#italic">e</hi>.</note>
                <note xml:id="p1746" type="edit" resp="scribe">MS <hi rend="#italic"
                    >stu</hi>.</note>
                <note xml:id="p1760" type="interp" resp="#SPM"><hi rend="#italic">già</hi>, from <hi
                        rend="#italic">gire</hi>, to go; "he did not go willing."</note>
                <note xml:id="p1766" type="ling" resp="#SPM">Modified for rhyme.</note>
                <note xml:id="p1773" type="edit" resp="scribe">MS <hi rend="#italic">vl</hi>;
                    scribal correction, with an <hi rend="#italic">o</hi> above the <hi
                        rend="#italic">v</hi>.</note>
                <note xml:id="p1780" type="codic" resp="#SPM">Catchwords <hi rend="#italic">lo
