                <note xml:id="p1307" type="edit" resp="#SPM">MS <hi rend="#italic"
                    >Sangui</hi>.</note>
                <note xml:id="p1345" type="codic" resp="#SPM">Space for illumination.</note>
                <note xml:id="p1368" type="edit" resp="scribe">MS <hi rend="#italic"
                    >inlperador</hi>; a cross in the right margin indicates that the scribe had
                    signaled a problem in this line, even if it is not clearly identified.</note>
                <note xml:id="p1371" type="edit" resp="scribe">MS <hi rend="#italic">rquier</hi>;
                    scribal correction with an <hi rend="#italic">e</hi> written above the <hi
                        rend="#italic">rq</hi>.</note>
                <note xml:id="p1399" type="edit" resp="#SPM">MS <hi rend="#italic">pre</hi></note>
                <note xml:id="p1433" type="edit" resp="#SPM">This line repeats.</note>
                <note xml:id="p1434" type="edit" resp="#SPM">MS <hi rend="#italic">Sico</hi>.</note>
                <note xml:id="p1450" type="ling" resp="#SPM"><hi rend="#italic">circondar</hi>,
                    metathesis.</note>
                <note xml:id="p1491" type="codic" resp="#SPM">Space for illumination; this line
                    repeats.</note>
                <!-- What happens when one note should have two types, as is the case for the above: codic and edit? -->
                <note xml:id="p1507" type="edit" resp="#SPM">MS <hi rend="#italic">no no
                    ve</hi>.</note>
                <note xml:id="p1511" type="edit" resp="scribe">MS <hi rend="#italic">odior</hi>;
                    scribal correction with <hi rend="#italic">e</hi> written above <hi
