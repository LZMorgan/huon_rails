                <note n="69" xml:id="p1266" type="edit" resp="#SPM">This line repeats.</note>
                <!-- Find way to insert the extra line for the diplomatic edition; .css should display the line in a different color. -->
                <note xml:id="p1274" type="edit" resp="#SPM">MS <hi rend="#italic">chavlcha</hi>;
                    scribal correction with <hi rend="#italic">a</hi> written above the <hi
                        rend="#italic">v</hi>.</note>
                <note xml:id="p1293" type="codic" resp="#SPM">Catchwords <hi rend="#italic">e si se
